    <DeployIisAppPath>FERRESYSNG</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$FERRESYSNG</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="FERRESYSNG.Models.ApplicationDbContext, FERRESYSNG" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="FERRESYSCOMMONTEST" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="DeventSoft.FERRESYS.DAL.COMMONDBContext, DeventSoft.FERRESYS.DAL" MigrationConfiguration="DeventSoft.FERRESYS.DAL.Migrations.Configuration, DeventSoft.FERRESYS.DAL" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="FERRESYS" Order="3" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=sql2005.shared-servers.com,1086;Initial Catalog=FERRESYSFRNTEST;User ID=ferresysfrh2;Password=Juan40202010" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\FERRESYS_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=sql2005.shared-servers.com,1086;Initial Catalog=FERRESYSFRNTEST;User ID=ferresysfrh2;Password=Juan40202010" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
